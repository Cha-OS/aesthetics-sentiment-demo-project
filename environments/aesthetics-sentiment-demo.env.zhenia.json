{
	"name": "aesthetics-sentiment-demo - Zhenia laptop",
	"authors": "Sasha Mile Rudan & Eugenia Kelbert",
	"version": "0.5",
	"description": "",

	"environment": {
		"ROOT": "BukvikRoot",
		"PROJECT_NAME": "aesthetics-sentiment-demo-project",
		"CORPORA_NAME": "aesthetics-sentiment-demo-corpus",
		"ETYMOLOGY_NAME": "bukvik-dictionary-etymology",
		"SUB_PROJECT_NAME": "lolita-en",
		"NAMESPACE_DATA": "aesthetics-sentiment-demo.data",
		"NAMESPACE_TASK": "aesthetics-sentiment-demo.task",
		"FOLDER_EXPERIMENTS": "../../..",
		"FOLDER_CACHE": "<FOLDER_EXPERIMENTS>/cache/<PROJECT_NAME>",
		"FOLDER_TEMP": "<FOLDER_EXPERIMENTS>/temp/<PROJECT_NAME>",
		"FOLDER_DATA": "<FOLDER_EXPERIMENTS>/datasets",
		"FOLDER_FLOWS": "../flows",
		"FOLDER_IN": "<FOLDER_DATA>/<CORPORA_NAME>",
		"FOLDER_OUT": "<FOLDER_DATA>/<PROJECT_NAME>",
		"FOLDER_ETYMOLOGY": "<FOLDER_DATA>/<ETYMOLOGY_NAME>",
		"TREE_TAGGER_PATH": "C:/data/programs/DH/TreeTagger/bin",
		"TREE_TAGGER_CMD_ENGLISH": "tag-english.bat",
		"TREE_TAGGER_CMD_RUSSIAN": "tag-russian.bat",
		"TREE_TAGGER_CMD_FRENCH": "tag-french.bat"
	},
	"folder_structure_example": {
		"experiments": {
			"projects": {
				"aesthetics-sentiment-demo-project": {
					"flows": true,
					"environments": true
				}
			},
			"cache": {
				"aesthetics-sentiment-demo-project": "(FOLDER_CACHE)"
			},
			"datasets": {
				"aesthetics-sentiment-demo-corpora": "(FOLDER_IN)",
				"aesthetics-sentiment-demo-project": "(FOLDER_OUT)"
			},
			"temp": {
				"aesthetics-sentiment-demo-project": "(FOLDER_TEMP)"
			}
		}
	}
}
